<?xml version="1.0"?>
<mavlink>
<include>common.xml</include>
<messages>
  <message name="MK_DEBUGOUT" id="100">
	 HUCH Mikrokopter DebugOut
	 <field name="debugout" type="array[66]">66 bytes of Debugout data. See qk_datatypes.h</field>
  </message>

  <message name="MK_EXTERN_CONTROL" id="101">
	 <field name="remote_buttons" type="uint8_t"></field>
	 <field name="nick" type="int8_t"></field>
	 <field name="roll" type="int8_t"></field>
	 <field name="yaw" type="int8_t"></field>
	 <field name="gas" type="uint16_t"></field>
	 <field name="height" type="uint16_t"></field>
	 <field name="AP_flags" type="uint8_t"></field>
	 <field name="frame" type="uint8_t"></field>
	 <field name="config" type="uint8_t"></field>
  </message>

  <message name="HUCH_EXP_CTRL" id="102">
	 <field name="status" type="uint8_t"></field>
	 <field name="rx" type="array[9]"></field>
	 <field name="tx" type="array[2]"></field>
  </message>

  <message name="HUCH_ATTITUDE" id="103">
	 <field name="xacc" type="int16_t"></field>
	 <field name="yacc" type="int16_t"></field>
	 <field name="zacc" type="int16_t"></field>
	 <field name="zaccraw" type="int16_t"></field>
	 <field name="xaccmean" type="int16_t"></field>
	 <field name="yaccmean" type="int16_t"></field>
	 <field name="zaccmean" type="int16_t"></field>
	 <field name="xgyro" type="int16_t"></field>
	 <field name="ygyro" type="int16_t"></field>
	 <field name="zgyro" type="int16_t"></field>
	 <field name="xgyroint" type="int32_t"></field>
	 <field name="ygyroint" type="int32_t"></field>
	 <field name="zgyroint" type="int32_t"></field>
	 <field name="xmag" type="int16_t"></field>
	 <field name="ymag" type="int16_t"></field>
	 <field name="zmag" type="int16_t"></field>
  </message>

  <message name="HUCH_FC_ALTITUDE" id="104">
	 <field name="baro" type="int16_t"></field>
	 <field name="baroref" type="int16_t"></field>
  </message>

  <message name="HUCH_RANGER" id="105">
	 <field name="ranger1" type="uint16_t"></field>
	 <field name="ranger2" type="uint16_t"></field>
	 <field name="ranger3" type="uint16_t"></field>
  </message>

  <message name="HUCH_EXP_CTRL_RX" id="107">
	 <field name="version" type="uint8_t"></field>
	 <field name="value0" type="uint16_t"></field>
	 <field name="value1" type="uint16_t"></field>
	 <field name="value2" type="uint16_t"></field>
	 <field name="value3" type="uint16_t"></field>
  </message>

<message name="MK_FC_STATUS" id="109">
	<field name="rssi" type="int16_t">RC Control RSSI on FlightCtrl</field>
	<field name="batt" type="int16_t">Battery Voltage Level</field>
	<field name="nick"  type="int16_t">FC internal nick value (steering.StickNick)</field>
	<field name="roll"  type="int16_t">FC internal roll value (steering.StickRoll)</field>
	<field name="yaw"  type="int16_t">FC internal yaw value (steering.StickYaw)</field>
	<field name="gas"  type="int16_t">FC internal gas value (GasMixFraction)</field>
</message>

<message name="HUCH_CTRL_HOVER_STATE" id="110">
	<field name="uss"  type="float">Ultrasonic range measurement in mm</field>
	<field name="baro" type="float">Barometric measurement in mm</field>
	<field name="accz" type="float">Accelerometer measurement in mm/s^2</field>
	<field name="ir1"  type="float">Infrared range measurement in mm</field>
	<field name="ir2"  type="float">Infrared range measurement in mm</field>
	<field name="kal_s0" type="float">Kalman state component 0 (pos)</field>
	<field name="kal_s1" type="float">Kalman state component 1 (vel)</field>
	<field name="kal_s2" type="float">Kalman state component 2 (acc)</field>
</message>

<message name="HUCH_IMU_RAW_ADC" id="111">
	<description>The RAW IMU ADC values, not calibrated, not scaled, not filtered!</description>
	<field name="xacc"  type="uint16_t">X acceleration (adc units)</field>
	<field name="yacc"  type="uint16_t">Y acceleration (adc units)</field>
	<field name="zacc"  type="uint16_t">Z acceleration (adc units)</field>
	<field name="xgyro" type="uint16_t">Angular speed around X axis (adc units)</field>
	<field name="ygyro" type="uint16_t">Angular speed around Y axis (adc units)</field>
	<field name="zgyro" type="uint16_t">Angular speed around Z axis (adc units)</field>
</message>

<message name="HUCH_MK_IMU" id="112">
	<description>The MikoKopter IMU values, (calibrated), scaled and filtered!</description>
	<field name="usec"  type="uint64_t">Timestamp (microseconds since UNIX epoch)</field>
	<field name="xacc"  type="int16_t">Calibrated X acceleration (mg)</field>
	<field name="yacc"  type="int16_t">Calibrated Y acceleration (mg)</field>
	<field name="zacc"  type="int16_t">Calibrated Z acceleration (mg)</field>
	<field name="xgyro" type="int16_t">Uncalibrated angular speed around X axis (0.1 deg/sec)</field>
	<field name="ygyro" type="int16_t">Uncalibrated angular speed around Y axis (0.1 deg/sec)</field>
	<field name="zgyro" type="int16_t">Uncalibrated angular speed around Z axis (0.1 deg/sec)</field>
</message>

<message name="HUCH_RAW_PRESSURE" id="113">
	<description>The RAW pressure readings for a pressure sensor in pascal</description>
	<field name="usec"  type="uint64_t">timestamp in microseconds</field>
	<field name="pressure"  type="int32_t">pressure in pascal</field>
</message>

<message name="HUCH_ALTITUDE" id="114">
	<description>The readings for an altitude sensor in meters</description>
	<field name="usec"  type="uint64_t">timestamp in microseconds</field>
	<field name="altitude"  type="float">absolute altitude in meter</field>
</message>

<message name="HUCH_TEMPERATURE" id="115">
	<description>The readings for an temperature sensor in meters</description>
	<field name="usec"  type="uint64_t">timestamp in microseconds</field>
	<field name="temperature"  type="int32_t">temperature in 0.1C</field>
</message>

<message name="HUCH_MAGNETIC_KOMPASS" id="116">
	<description>The readings for a 3D magnetic field sensor in Gauss</description>
	<field name="usec"  type="uint64_t">timestamp in microseconds</field>
	<field name="data_x" type="float">magnetic field x-direction in Gs</field>
	<field name="data_y" type="float">magnetic field y-direction in Gs</field>
	<field name="data_z" type="float">magnetic field z-direction in Gs</field>
</message>

<message name="HUCH_DISTANCE" id="117">
	<description>The readings for an distance sensor in meters</description>
	<field name="usec" type="uint64_t">timestamp in microseconds</field>
	<field name="distance" type="float">relativ distance in meter</field>
</message>

<message name="HUCH_WARPING" id="118">
	<description>Warping output</description>
	<field name="alpha" type="float">warping movement direction</field>
	<field name="psi" type="float">warping compass</field>
	<field name="rho" type="float">warping relative distance</field>
	<field name="distance" type="int32_t">minimal image distance</field>
	<field name="psi_estimate" type="float">compass estimate</field>
	<field name="beta" type="float">home direction</field>
</message>

<message name="HUCH_ANALOG" id="119">
	<description>The readings for an analog sensor in meters</description>
	<field name="usec"  type="uint64_t">timestamp in microseconds</field>
	<field name="analog"  type="int32_t">analog value in ADC units</field>
</message>

<message name="HUCH_HC_RAW" id="120">
	<description>hover control: raw altitude readings</description>
	<field name="usec"  type="uint64_t">timestamp in microseconds</field>
	<field name="raw0"  type="int32_t">raw altitude readings for logging and playback to controller</field>
	<field name="raw1"  type="int32_t">raw altitude readings for logging and playback to controller</field>
	<field name="raw2"  type="int32_t">raw altitude readings for logging and playback to controller</field>
	<field name="raw3"  type="int32_t">raw altitude readings for logging and playback to controller</field>
	<field name="raw4"  type="int32_t">raw altitude readings for logging and playback to controller</field>
</message>

